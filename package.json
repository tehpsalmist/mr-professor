  "devDependencies": {
    "@parcel/transformer-svg-react": "^2.12.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "autoprefixer": "^10.4.19",
    "parcel": "^2.12.0",
